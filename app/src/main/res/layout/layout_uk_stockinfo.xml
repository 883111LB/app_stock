        </FrameLayout>
        <FrameLayout
            android:padding="@dimen/dimen_3"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:text="年最高价日期(非自动):"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_11"
                android:textColor="?attr/item_main_text_color"/>
            <TextView
                android:id="@+id/tev_highPriceTimeYearNonAuto"
                android:layout_gravity="right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_11"
                android:textColor="?attr/item_main_text_color"/>
        </FrameLayout>
        <FrameLayout
            android:padding="@dimen/dimen_3"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:text="年最低价日期(非自动):"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_11"
                android:textColor="?attr/item_main_text_color"/>
            <TextView
                android:id="@+id/tev_lowPriceTimeYearNonAuto"
                android:layout_gravity="right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_11"
                android:textColor="?attr/item_main_text_color"/>
        </FrameLayout>
        <FrameLayout
            android:padding="@dimen/dimen_3"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:text="年最高价(非自动):"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_11"
                android:textColor="?attr/item_main_text_color"/>
            <TextView
                android:id="@+id/tev_highPriceYearNonAuto"
                android:layout_gravity="right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_11"
                android:textColor="?attr/item_main_text_color"/>
        </FrameLayout>
        <FrameLayout
            android:padding="@dimen/dimen_3"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:text="年最低价(非自动):"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_11"
                android:textColor="?attr/item_main_text_color"/>
            <TextView
                android:id="@+id/tev_lowPriceYearNonAuto"
                android:layout_gravity="right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_11"
                android:textColor="?attr/item_main_text_color"/>
        </FrameLayout>\
        <FrameLayout
            android:padding="@dimen/dimen_3"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:text="币种:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_11"
                android:textColor="?attr/item_main_text_color"/>
            <TextView
                android:id="@+id/tev_currency"
                android:layout_gravity="right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/font_11"
                android:textColor="?attr/item_main_text_color"/>
