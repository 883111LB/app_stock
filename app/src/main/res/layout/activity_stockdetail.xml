<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="?attr/background_color"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.cvicse.stock.base.ToolBar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:text_title="自选"
        app:filtercolor="?attr/filtercolor_toolbar"
        app:src_left="@drawable/common_return_back_arrow"
        app:src_right_1="@drawable/bg_addselect_seletor"
        app:src_right_2="@drawable/navigationbar_button_search"
        android:background="?attr/bg_toolbar" />

    <android.support.v4.view.ViewPager
        android:id="@+id/vp_stockdetail"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>